<%- include('../partials/header') %>

<h1>Toiletry</h1>

<tbody>
  <% toiletry.forEach(function(item) { %>
    <tr>
      <td><%= item.brand %></td>
      <td><%= item.productName %></td>
     <img src="<%= item.imageURL %>" alt="img">
     <a href="/show/toiletry/<%=item._id %>">See Details</a>
    </tr>
    <% }); %>
  </tbody>

<%- include('../partials/footer') %>
