<%- include('../partials/header') %>

<section id="items">
  <% toiletry.forEach(function(item) { %>
    <article class="card">
    <h3><%= item.brand %></h3>
    <h4><%= item.productName %></h4>
     <img src="<%= item.imageURL %>" alt="img">
     <a href="/show/toiletry/<%=item._id %>">See Details</a>
    </article>
    <% }); %>
</section>

<%- include('../partials/footer') %>
