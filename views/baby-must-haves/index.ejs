<%- include('../partials/header') %>
<h1>Baby-Must-Haves</h1>

<tbody>
  <% babyMustHave.forEach(function(item) { %>
    <tr>
      <td><%= item.brand %></td>
      <td><%= item.productName %></td>
     <img src="<%= item.imageURL %>" alt="img">
     <a href="/show/baby-must-haves/<%=item._id %>">See Details</a>
    </tr>
    <% }); %>
  </tbody>

<%- include('../partials/footer') %>
