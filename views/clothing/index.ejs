<%- include('../partials/header') %>

<!-- <h2>Clothing</h2> -->

<div id="container">
  <section id="items">
    <% clothing.forEach(function(item) { %>
      <article class="card">
      <h3><%= item.brand %></h3>
      <h4><%= item.productName %></h4>
       <img src="<%= item.imageURL %>" alt="img">
       <a href="/show/clothing/<%=item._id %>">See Details</a>
      </article>
      <% }); %>
  </section>
</div>


<%- include('../partials/footer') %>
