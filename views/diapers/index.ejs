<%- include('../partials/header') %>

<h1>Diapers</h1>

<tbody>
  <% diaper.forEach(function(item) { %>
    <tr>
      <td><%= item.brand %></td>
      <td><%= item.productName %></td>
     <img src="<%= item.imageURL %>" alt="img">
     <a href="/show/diapers/<%=item._id %>">See Details</a>
    </tr>
    <% }); %>
  </tbody>

<%- include('../partials/footer') %>
